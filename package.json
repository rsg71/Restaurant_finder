  "main": "server.ts",
  "scripts": {
    "start": "node build/server.js | pino-pretty --colorize",
    "copyPublicFolder": "copyfiles -u 1 public/**/* build/public",
    "copyViewsFolder": "copyfiles -u 1 views/**/* build/views",
    "postinstall": "npm run build",
    "start:dev": "npx nodemon -L server.ts | pino-pretty --colorize",
    "start:backend_dev": "npx nodemon -L server.ts | pino-pretty --colorize",
    "start:whole_app_dev": "set NODE_ENV=development&& concurrently \"nodemon --ignore 'frontend/*'\" \"npm run frontend\" | pino-pretty",
    "frontend": "cd frontend && npm run start",
    "start:frontend_dev": "cd frontend && npm run start",
    "test": "npm run lint",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix .",
    "watch": "nodemon server.js | pino-pretty",
    "install": "cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
